"use strict";var BookingAppFilters=angular.module("BookingApp.filters");BookingAppFilters.filter("money",function(){return function(num,cutZeros,addSpaces){return formatMoney(num,void 0,cutZeros,addSpaces)}});var BookingAppFilters=angular.module("BookingApp.filters");BookingAppFilters.filter("moneyFc",function(){return function(num,cutZeros,addSpaces){return formatMoneyFc(num,void 0,cutZeros,addSpaces)}});var BookingAppFilters=angular.module("BookingApp.filters");BookingAppFilters.filter("month",function(){var monthNames=["Январь","Февраль","Март","Апрель","Май","Июнь","Июль","Август","Сентябрь","Октябрь","Ноябрь","Декабрь"];return function(object){var month=object instanceof Date?object.getMonth():object;return month<0?month+=12:month>11&&(month-=12),monthNames[month]}});var BookingAppFilters=angular.module("BookingApp.filters");BookingAppFilters.filter("phone",["$filter",function($filter){return function(phone,mask){if(!phone)return"";if(phone=phone.replace(/\D/g,""),!mask)return phone;for(var result="",phoneIndex=0,i=0;i<mask.length;i++){var bukva=mask[i];result+="#"===bukva?phone[phoneIndex++]:bukva}return result}}]);